<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <application>
      <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    
    <component>
		<component-type>org.primefaces.component.SentinelMenu</component-type>
		<component-class>org.primefaces.sentinel.component.menu.SentinelMenu</component-class>
	</component>
    
    <render-kit>
        <renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.SentinelMenuRenderer</renderer-type>
			<renderer-class>org.primefaces.sentinel.component.menu.SentinelMenuRenderer</renderer-class>
		</renderer>
    </render-kit>


	<navigation-rule>
         <from-view-id>/*</from-view-id>
         <navigation-case>
             <from-outcome>dashboard</from-outcome>
             <to-view-id>/dashboard.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>promotion-page</from-outcome>
             <to-view-id>/promotion-page.xhtml</to-view-id>               
         </navigation-case>
         <navigation-case>
             <from-outcome>sample</from-outcome>
             <to-view-id>/sample.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>forms</from-outcome>
             <to-view-id>/forms.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>data</from-outcome>
             <to-view-id>/data.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>datatable</from-outcome>
             <to-view-id>/datatable.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>panel</from-outcome>
             <to-view-id>/panel.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>overlay</from-outcome>
             <to-view-id>/overlay.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>menu</from-outcome>
             <to-view-id>/menu.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>messages</from-outcome>
             <to-view-id>/messages.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>charts</from-outcome>
             <to-view-id>/charts.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>file</from-outcome>
             <to-view-id>/file.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>misc</from-outcome>
             <to-view-id>/misc.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>empty-page</from-outcome>
             <to-view-id>/empty-page.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>login</from-outcome>
             <to-view-id>/login.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>error</from-outcome>
             <to-view-id>/error.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>404</from-outcome>
             <to-view-id>/404.xhtml</to-view-id>               
         </navigation-case>
          <navigation-case>
             <from-outcome>access-denied</from-outcome>
             <to-view-id>/access-denied.xhtml</to-view-id>               
         </navigation-case>         
         <navigation-case>
             <from-outcome>font-icons.xhtml</from-outcome>
             <to-view-id>/font-icons.xhtml</to-view-id>               
         </navigation-case>  
          <navigation-case>
             <from-outcome>documentation</from-outcome>
             <to-view-id>/documentation.xhtml</to-view-id>               
         </navigation-case>
         
         <navigation-case>
             <from-outcome>vtEmpresa</from-outcome>
             <to-view-id>/XHTML/vtEmpresa.xhtml</to-view-id>               
         </navigation-case>
         
         <navigation-case>
             <from-outcome>vtEmpresaListDataTable</from-outcome>
             <to-view-id>/XHTML/vtEmpresaListDataTable.xhtml</to-view-id>               
         </navigation-case>
         
         <navigation-case>
             <from-outcome>vtEmpresaListDataTableEditable</from-outcome>
             <to-view-id>/XHTML/vtEmpresaListDataTableEditable.xhtml</to-view-id>               
         </navigation-case>
         
         
         
         
         
    </navigation-rule>
	
</faces-config>
